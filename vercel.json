{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://api-satusehat-dev.dto.kemkes.go.id/fhir-r4/v1/:path*",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST, OPTIONS",
        "Access-Control-Allow-Headers": "Authorization, Content-Type"
      }
    }
  ]
}
